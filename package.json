{
  "name": "js-spec",
  "version": "0.0.0",
  "description": "A simple, yet all-inclusive unit test framework with built-in mocking and assertions with Typescript support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mhweiner/js-spec.git"
  },
  "scripts": {
    "test": "nyc tape -r ts-node/register ./**/*.spec.ts | tap-spec && nyc report --reporter=text",
    "lint": "eslint ./ --ext .js,.ts",
    "build": "rm -rf ./dist && tsc --project ./tsconfig.json",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/mhweiner/js-spec",
  "keywords": [
    "unit test",
    "ts unit test",
    "typescript unit test",
    "unit-test",
    "mock",
    "mocking",
    "typescript mocking",
    "ts mock",
    "proxyquire",
    "ts proxyquire",
    "typescript proxyquire",
    "isolate",
    "dependency mock",
    "rewire"
  ],
  "dependencies": {
    "c8": "^7.10.0",
    "callsites": "4.0.0",
    "execa": "5.1.1",
    "fast-glob": "^3.2.7",
    "nyc": "15.1.0",
    "serialize-error": "8.1.0",
    "sinon": "11.1.2",
    "tap-spec": "5.0.0",
    "tape": "5.3.1",
    "timekeeper": "2.2.0"
  },
  "devDependencies": {
    "@types/execa": "2.0.0",
    "@types/glob": "7.2.0",
    "@types/node": "^16.11.6",
    "@types/sinon": "10.0.6",
    "@types/tape": "4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "glob": "7.2.0",
    "semantic-release": "^18.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
